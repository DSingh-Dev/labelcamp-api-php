{
    "name": "bhanwarpsrathore/labelcamp-api-php",
    "description": "A PHP wrapper for LabelCamp API.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "labelcamp"
    ],
    "homepage": "https://github.com/bhanwarpsrathore/labelcamp-api-php",
    "authors": [
        {
            "name": "Pradeep Singh",
            "email": "bhanwarpsrathore@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "graham-campbell/guzzle-factory": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.30"
    },
    "autoload": {
        "psr-4": {
            "LabelcampAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LabelcampAPI\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
